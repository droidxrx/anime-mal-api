{
    "name": "anime-mal-api",
    "version": "1.0.10",
    "description": "An awesome wrapper on Nodejs for the new MyAnimeList's API v2!",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/droidxrx/anime-mal-api.git"
    },
    "keywords": [
        "mal",
        "mal-api",
        "mal-token",
        "mal-token-generate",
        "pkce"
    ],
    "author": "DroidXrX <droidxrx@gmail.com> (https://github.com/droidxrx)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/droidxrx/anime-mal-api/issues"
    },
    "homepage": "https://github.com/droidxrx/anime-mal-api#readme",
    "scripts": {
        "publish": "npm version patch && clean-publish",
        "server": "node ./example/server.js",
        "tscomple": "rmdir /s /q dist && tsc -p tsconfig.json && npm run tsd",
        "tsd": "dts-bundle-generator --config dtsgenerate.json"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.0.2",
        "@types/node": "^16.6.1",
        "@types/superagent": "^4.1.12",
        "clean-publish": "^2.2.0",
        "cors": "^2.8.5",
        "dts-bundle-generator": "^5.9.0",
        "express": "^4.17.1"
    },
    "dependencies": {
        "crypto-js": "^4.1.1",
        "superagent": "^6.1.0"
    },
    "clean-publish": {
        "files": [
            ".git",
            ".vscode",
            "example",
            "node_modules",
            "typescript",
            "test",
            "package-lock.json",
            "tsconfig.json",
            "dtsgenerate.json",
            "dtsconfig.json"
        ],
        "fields": [
            "scripts",
            "devDependencies",
            "clean-publish"
        ]
    }
}
