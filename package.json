{
    "name": "anime-mal-api",
    "version": "1.0.0",
    "description": "An awesome wrapper on Nodejs for the new MyAnimeList's API v2!",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/droidxrx/anime-mal-api.git"
    },
    "keywords": [
        "mal",
        "mal-api",
        "mal-token",
        "mal-token-generate",
        "pkce"
    ],
    "author": "DroidXrX <droidxrx@gmail.com> (https://github.com/droidxrx)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/droidxrx/anime-mal-api/issues"
    },
    "homepage": "https://github.com/droidxrx/anime-mal-api#readme",
    "scripts": {
        "publish": "clean-publish",
        "parcel": "parcel build ./src/node.ts --no-source-maps --no-cache --no-content-hash --no-scope-hoist --no-optimize --dist-dir ./lib && minify.cmd lib",
        "parcelbrowser": "parcel build ./src/browser.html --no-source-maps --no-cache --no-content-hash --no-scope-hoist --no-optimize --dist-dir ./dist && minify.cmd dist"
    },
    "dependencies": {
        "crypto-js": "^4.1.1",
        "superagent": "^6.1.0"
    },
    "clean-publish": {
        "files": [
            ".git",
            ".gitignore",
            ".vscode",
            "example",
            "node_modules",
            "typescript",
            "test",
            "package-lock.json",
            "tsconfig.json",
            "dtsgenerate.json",
            "dtsconfig.json",
            "src",
            "minify.cmd"
        ],
        "fields": [
            "scripts",
            "devDependencies",
            "clean-publish"
        ]
    }
}
